<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".InClass_07.InClass07_main">

    <Button
        android:id="@+id/InClass07_buttonToRegisterPage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:text="Register"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="263dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/InClass07_buttonToLoginPage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:text="Login"
        app:layout_constraintEnd_toEndOf="@+id/InClass07_buttonToRegisterPage"
        app:layout_constraintStart_toStartOf="@+id/InClass07_buttonToRegisterPage"
        app:layout_constraintTop_toBottomOf="@+id/InClass07_buttonToRegisterPage" />
</androidx.constraintlayout.widget.ConstraintLayout>